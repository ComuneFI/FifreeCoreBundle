{
    "name": "fi/fifreecorebundle",
    "description": "Core Fifree2",
    "type": "project",
    "authors": [{
            "name": "Andrea Manzi",
            "email": "andrea.manzi@comune.fi.it"
        },
        {
            "name": "Emidio Picariello",
            "email": "emidio.picariello@comune.fi.it"
        },
        {
            "name": "Angela Bianchi",
            "email": "angela.bianchi@comune.fi.it"
        },
        {
            "name": "Lisa Pescini",
            "email": "lisa.pescini@comune.fi.it"
        },
        {
            "name": "Paolo Pescini",
            "email": "paolo.pescini@comune.fi.it"
        }],
    "keywords": [
        "Core fifree2 bundle"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Fi\\CoreBundle\\": "src/Fi/CoreBundle",
            "Fi\\PannelloAmministrazioneBundle\\": "src/Fi/PannelloAmministrazioneBundle",
            "Fi\\ProvaBundle\\": "src/Fi/ProvaBundle"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "require": {
        "php": ">=5.6.0",
        "symfony/symfony": "3.4.0-BETA2 as 3.4.0",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "symfony/swiftmailer-bundle": "^2.4",
        "symfony/monolog-bundle": "^3.0",
        "sensio/distribution-bundle": "~5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "^2.0",
        "twig/twig": "~1.6",
        "friendsofsymfony/user-bundle": "~2.0",
        "fi/osbundle": "~2.0",
        "mysql-workbench-schema-exporter/mysql-workbench-schema-exporter": "^3.0",
        "mysql-workbench-schema-exporter/doctrine2-exporter": "^3.0",
        "tecnickcom/tcpdf": "^6.2",
        "phpoffice/phpexcel": "^1.8",
        "phpoffice/phpword": "^0.13.0"
        
    },
    "require-dev": {
        "sensio/generator-bundle": "~3.0",
        "sensiolabs/security-checker": "^3.0",
        "satooshi/php-coveralls": "^1.0",
        "squizlabs/php_codesniffer": "^2.7",
        "phpmd/phpmd": "^2.4",
        "sebastian/phpcpd": "^2.0",
        "mockery/mockery": "^0.9.5",
        "behat/behat": "^3.0",
        "behat/mink-selenium2-driver": "^1.3",
        "behat/symfony2-extension": "^2.0",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-extension": "^2.2",
        "behat/mink-goutte-driver": "^1.2",
        "symfony/web-profiler-bundle": "^3.3",
        "se/selenium-server-standalone": "^2.0",
        "phpunit/phpunit": "^5.5",
        "symfony/web-server-bundle": "v3.4.0-BETA2"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml",
            "keep-outdated": true
        },
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
