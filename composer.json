{
    "name": "fi/fifreecorebundle",
    "description": "Core Fifree2",
    "type": "project",
    "authors": [{
            "name": "Andrea Manzi",
            "email": "andrea.manzi@comune.fi.it"
        },
        {
            "name": "Emidio Picariello",
            "email": "emidio.picariello@comune.fi.it"
        },
        {
            "name": "Angela Bianchi",
            "email": "angela.bianchi@comune.fi.it"
        },
        {
            "name": "Lisa Pescini",
            "email": "lisa.pescini@comune.fi.it"
        },
        {
            "name": "Paolo Pescini",
            "email": "paolo.pescini@comune.fi.it"
        }],
    "keywords": [
        "Core fifree2 bundle"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Fi\\CoreBundle\\": "src/Fi/CoreBundle",
            "Fi\\PannelloAmministrazioneBundle\\": "src/Fi/PannelloAmministrazioneBundle",
            "Fi\\ProvaBundle\\": "tests/src/Fi/ProvaBundle"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "classmap": [
            "tests/app/AppKernel.php",
            "tests/app/AppCache.php"
        ],
        "files": [
            "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php"
        ]
    },
    "require": {
        "php": ">=5.6.0",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.5",
        "fi/osbundle": "~2.0",
        "mysql-workbench-schema-exporter/doctrine2-exporter": "^3.0",
        "mysql-workbench-schema-exporter/mysql-workbench-schema-exporter": "^3.0",
        "incenteev/composer-parameter-handler": "^2.0",
        "sensio/distribution-bundle": "~5.0",
        "sensio/framework-extra-bundle": "^5.0.0",
        "symfony/swiftmailer-bundle": "^2.4",
        "symfony/monolog-bundle": "^3.1.0",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/symfony": "3.4.*",
        "twig/twig": "^1.0||^2.0",
        "friendsofsymfony/user-bundle": "~2.0"
    },
    "require-dev": {
        "phpoffice/phpexcel": "^1.8",
        "phpoffice/phpword": "^0.13.0",
        "tecnickcom/tcpdf": "^6.2",
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^4.0",
        "symfony/web-server-bundle": "3.4.*",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-selenium2-driver": "^1.3",
        "behat/symfony2-extension": "^2.1",
        "satooshi/php-coveralls": "^2.0",
        "mockery/mockery": "^0.9.5",
        "phpmd/phpmd": "^2.4",
        "sebastian/phpcpd": "^2.0",
        "sensiolabs/security-checker": "^4.0",
        "squizlabs/php_codesniffer": "^2.7",
        "se/selenium-server-standalone": "^2.0"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "bin": ["check-src"],
    "extra": {
        "symfony-app-dir": "tests/app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "tests/app/config/parameters.yml",
            "keep-outdated": true
        },
        "branch-alias": {
            "dev-master": "3.4-dev"
        }
    }
}
