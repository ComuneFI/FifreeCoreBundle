{
    "name": "fi/fifreecorebundle",
    "description": "Core Fifree2",
    "type": "project",
    "authors": [{
            "name": "Andrea Manzi",
            "email": "andrea.manzi@comune.fi.it"
        },
        {
            "name": "Emidio Picariello",
            "email": "emidio.picariello@comune.fi.it"
        },
        {
            "name": "Angela Bianchi",
            "email": "angela.bianchi@comune.fi.it"
        },
        {
            "name": "Lisa Pescini",
            "email": "lisa.pescini@comune.fi.it"
        },
        {
            "name": "Paolo Pescini",
            "email": "paolo.pescini@comune.fi.it"
        }],
    "keywords": [
        "Core fifree2 bundle"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "": "tests/src",
            "Fi\\CoreBundle\\": "src/Fi/CoreBundle",
            "Fi\\PannelloAmministrazioneBundle\\": "src/Fi/PannelloAmministrazioneBundle"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Fi\\ProvaBundle\\": "tests/src/Fi/ProvaBundle"
        },
        "classmap": [
            "tests/app/Kernel.php"
        ]
    },
    "require": {
        "php": ">=7.2",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.5",
        "fi/osbundle": "~2.0",
        "friendsofsymfony/user-bundle": "~2.0",
        "mysql-workbench-schema-exporter/doctrine2-exporter": "^3.0",
        "mysql-workbench-schema-exporter/mysql-workbench-schema-exporter": "^3.0",
        "sensio/distribution-bundle": "^5.0.19",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/asset": "3.4.*",
        "symfony/console": "3.4.*",
        "symfony/dotenv": "3.4.*",
        "symfony/flex": "^1.0",
        "symfony/form": "3.4.*",
        "symfony/framework-bundle": "3.4.*",
        "symfony/lock": "3.4.*",
        "symfony/monolog-bundle": "^3.2",
        "symfony/orm-pack": "^1.0",
        "symfony/security-bundle": "3.4.*",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/translation": "3.4.*",
        "symfony/twig-bundle": "3.4.*",
        "symfony/validator": "3.4.*"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "enm1989/chromedriver": "^2.35",
        "enm1989/geckodriver": "^0.19.1",
        "facebook/webdriver": "^1.5",
        "mockery/mockery": "^0.9.5",
        "phpmd/phpmd": "^2.6",
        "phpoffice/phpexcel": "^1.8",
        "phpoffice/phpword": "^0.13.0",
        "satooshi/php-coveralls": "^2.0",
        "se/selenium-server-standalone": "3.8.*",
        "sebastian/phpcpd": "^2.0",
        "sensiolabs/security-checker": "^4.0",
        "squizlabs/php_codesniffer": "^2.7",
        "symfony/browser-kit": "3.4.*",
        "symfony/css-selector": "3.4.*",
        "symfony/maker-bundle": "^1.1",
        "symfony/phpunit-bridge": "^3.0|^4.0",
        "symfony/profiler-pack": "^1.0",
        "symfony/var-dumper": "3.4.*",
        "symfony/web-server-bundle": "3.4.*",
        "tecnickcom/tcpdf": "^6.2"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "suggest": {
        "ext-apcu": "The extension required to use cache"
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative tests/%PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check --end-point=http://security.sensiolabs.org/check_lock": "script"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "bin": ["check-src"],
    "extra": {
        "symfony-app-dir": "tests/app",
        "symfony-bin-dir": "tests/bin",
        "symfony-web-dir": "tests/web",
        "symfony-tests-dir": "tests",
        "branch-alias": {
            "dev-master": "3.4-dev"
        }
    }
}
